{
  "name": "supertest-jest",
  "version": "1.0.0",
  "description": "This is a Restful service testing framework using Supertest and Jest",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "ASC"
  ],
  "author": "A madamombe",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@types/jest": "^29.5.3",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.2",
    "jest-cli": "^29.6.2",
    "jest-html-reporter": "^3.10.1",
    "superagent": "^8.0.9",
    "superagent-retry": "^0.6.0",
    "supertest": "^6.3.3",
    "typescript": "^5.1.6"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report",
          "outputPath": "Report/index.html",
          "includeFailureMsg": true
        }
      ]
    ]
  }
}
